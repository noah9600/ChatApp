{
    "version": 2,
    "builds": [
      {
        "src": "backend/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/socket.io",
        "dest": "backend/server.js"
      },
      {
        "src": "/.*",
        "dest": "public/index.html"
      }
    ]
  }
  